provider "aws" {
version = "3.27.0"
 region = var.region
 access_key = var.mainaccess_key
 secret_key = var.mainsecret_key
}

variable "name" {
default = "demo"

}
variable "path" {
default = "/system/"
}

variable "region" {
default = "eu-west-1"
}

variable "mainaccess_key" {
default = ""
}

variable "mainsecret_key" {
default = ""
}


variable "pgp_key" {
default = ""
}
